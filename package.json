{
  "name": "sistema-de-notas",
  "version": "1.1.1",
  "description": "A minimal Electron application written with Typescript",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "breact": "react-scripts build",
    "react": "react-scripts start",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./public/electron.js",
    "dist": "npm run breact && npm run build &&  electron-builder"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@types/node": "^18.7.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^17.0.1",
    "electron-builder": "^23.3.3",
    "eslint": "^7.32.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.10.11",
    "@mui/x-data-grid": "^5.17.9",
    "@mui/x-date-pickers": "^5.0.6",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "chart.js": "^3.9.1",
    "electron-is-dev": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.0",
    "reflect-metadata": "^0.1.13",
    "sweetalert2": "^11.4.4",
    "typeorm": "^0.3.7",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.sistema.de.notas",
    "productName": "sistema de notas",
    "copyright": "Copyright Â© 2022 Manuel Puente",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "public/img/logo.png"
    }
  }
}
