{
  "name": "sistema-de-notas",
  "version": "1.1.1",
  "description": "A minimal Electron application written with Typescript",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "breact": "react-scripts build",
    "react": "react-scripts start",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./public/electron.js"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^17.1.0",
    "eslint": "^8.10.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "chart.js": "^3.7.1",
    "electron-is-dev": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "sweetalert2": "^11.4.4",
    "typeorm": "^0.2.44",
    "typeorm-extension": "^1.2.0",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.sistema.de.notas",
    "productName": "sistema de notas",
    "copyright": "Copyright Â© 2022 Manuel Puente",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "public/img/logo.png"
    }
  }
}
